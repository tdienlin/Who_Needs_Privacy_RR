@article{peterAdolescentsPornographyReview2016,
  title = {Adolescents and Pornography: {{A}} Review of 20 Years of Research},
  author = {Peter, Jochen and Valkenburg, Patti M.},
  date = {2016-01-01T00:00:00},
  journaltitle = {Journal of sex research},
  shortjournal = {J Sex Res},
  volume = {53},
  number = {4-5},
  eprint = {27105446},
  eprinttype = {pmid},
  pages = {509--31},
  issn = {0022-4499},
  doi = {10.1080/00224499.2016.1143441},
  abstract = {The goal of this review was to systematize empirical research that was published in peer-reviewed English-language journals between 1995 and 2015 on the prevalence, predictors, and implications of adolescents' use of pornography. This research showed that adolescents use pornography, but prevalence rates varied greatly. Adolescents who used pornography more frequently were male, at a more advanced pubertal stage, sensation seekers, and had weak or troubled family relations. Pornography use was associated with more permissive sexual attitudes and tended to be linked with stronger gender-stereotypical sexual beliefs. It also seemed to be related to the occurrence of sexual intercourse, greater experience with casual sex behavior, and more sexual aggression, both in terms of perpetration and victimization. The findings of this review need to be seen against the background of various methodological and theoretical shortcomings, as well as several biases in the literature, which currently precludes internally valid causal conclusions about effects of pornography on adolescents.}
}

@article{peterEmpfehlungenUmgangMit2020,
  title = {Empfehlungen Zum {{Umgang}} Mit {{Forschungsdaten}} in Der {{Kommunikationswissenschaft}} – {{AG Forschungsdaten}} Im {{Auftrag}} Des {{Vorstands}} Der {{DGPuK}}},
  author = {Peter, Christina and Breuer, Johannes and Masur, Philipp K. and Scharkow, Michael and Schwarzenegger, Christian},
  date = {2020},
  journaltitle = {Studies in Communication and Media},
  shortjournal = {SCM},
  volume = {9},
  number = {4},
  pages = {599--626},
  issn = {2192-4007},
  doi = {10.5771/2192-4007-2020-4-599},
  url = {https://www.nomos-elibrary.de/index.php?doi=10.5771/2192-4007-2020-4-599},
  urldate = {2021-10-14},
  abstract = {Forschungsdaten sind das primäre Produkt empirischer Studien und die zentrale Grundlage wissenschaftlicher Erkenntnis. Ihre langfristige Sicherung und Bereitstellung zur intersubjektiven Nachvollziehbarkeit und damit zur Qualitätssicherung sollte daher in allen wissenschaftlichen Disziplinen unterstützt werden. Entsprechend hat die Deutsche Forschungsgemeinschaft 2016 die sozialwissenschaftlichen Fachgesellschaften in Deutschland aufgefordert, sich mit dem Thema zu befassen und konkrete Leitlinien zum Umgang mit Forschungsdaten zu entwickeln. Die vorliegenden Leitlinien sind dabei aus einer Arbeitsgruppe der Deutschen Gesellschaft für Publizistik- und Kommunikationswissenschaft entstanden. Sie beziehen sich auf alle in der Kommunikationswissenschaft generierten Datentypen und reflektieren dabei die Bedeutung der Bereitstellung von Forschungsdaten, aber auch Herausforderungen und Einschränkungen. Daraus werden Empfehlungen für verschiedene Zielgruppen abgeleitet, um die Bereitstellung von Forschungsdaten in der Kommunikationswissenschaft stärker zu fördern.           ,              Research data are the primary product of empirical studies and essential for generating scientific knowledge. Their accessibility and long-term preservation should therefore be encouraged by all scientific disciplines. Accordingly, the German Research Foundation asked the social sciences associations in Germany to address the topic and develop guidelines for the handling of research data in their discipline. A working group of the German Communication Association (DGPuK) developed the present guidelines. They refer to all data types generated in communication science and reflect the importance of making research data available, while also taking into account challenges and limitations. Recommendations for different target groups are provided to promote the availability of research data in communication science.},
  file = {/Users/Dienlin/Zotero/storage/AEZVK7BW/Peter et al. - 2020 - Empfehlungen zum Umgang mit Forschungsdaten in der.pdf}
}

@article{petersenSystematischeUndHeuristische1997,
  title = {Systematische Und Heuristische {{Informationsverarbeitung}} Beim {{Betrachten}} Einer {{Infotainmentsendung}}},
  author = {Petersen, Lars-Eric and Doll, Jörg and Jürgensen, Silke},
  date = {1997-01-01},
  journaltitle = {Journal of Media Psychology Theories Methods and Applications},
  shortjournal = {Journal of Media Psychology Theories Methods and Applications},
  volume = {9},
  pages = {24--40},
  abstract = {Systematic and heuristic information processing while watching an infotainment programm The present study examines how an infotainment program influences the attitude formation of persons watching the program to an object presented in the program. The current television viewing habit of television as an accompanying activity is also considered. The design of the study is derived from the elaboration likelihood model which has been formulated within attitude research. Subjects watched part of the ZDF-program "Das Literarische Quartett" in which four critics discuss a recent book publication. Two of them voted with strong arguments for the book, the other two with weak arguments against it. Subjects either watched the program without or with distraction (television as accompanying activity). Additionally, one pair of critics was introduced as competent and the other pair as incompetent. Subjects formed their attitudes to the discussed book in accordance with the competence-priming not only under high distraction (as expected), but also under low distraction. Under both conditions subjects used a heuristic information processing strategy and derived their attitudes primarily from the primed information about the critics. It is assumed that infotainment programs prevent a systematic processing of the presented information by their personalizing and emotionalizing character. Only the processing of the program moderator's argumentation made a difference. The moderator's argumentation is processed systematically under low distraction because its quality is much lower than it would have been expected considering the moderator's generally high expertness.}
}

