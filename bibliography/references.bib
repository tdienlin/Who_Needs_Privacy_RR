  keywords = {Innovative Methoden}
}

@article{valenteCausalMediationPrograms2020,
  title = {Causal {{Mediation Programs}} in {{R}}, {{Mplus}}, {{SAS}}, {{SPSS}}, and {{Stata}}},
  author = {Valente, Matthew J. and Rijnhart, Judith J. M. and Smyth, Heather L. and Muniz, Felix B. and MacKinnon, David P.},
