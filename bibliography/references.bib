@article{sedikidesNostalgiaPresentFuture2008,
  title = {Nostalgia: {{Past}}, {{Present}}, and {{Future}}},
  author = {Sedikides, Constantine and Wildschut, Tim and Arndt, Jamie and Routledge, Clay},
  date = {2008-01-01T00:00:00},
  journaltitle = {Current Directions in Psychological Science},
  shortjournal = {Curr Dir Psychol Sci},
  volume = {17},
  number = {5},
  pages = {304307},
  issn = {0963-7214},
  doi = {10.1111/j.1467-8721.2008.00595.x},
  abstract = {Traditionally, nostalgia has been conceptualized as a medical disease and a psychiatric disorder. Instead, we argue that nostalgia is a predominantly positive, self-relevant, and social emotion serving key psychological functions. Nostalgic narratives reflect more positive than negative affect, feature the self as the protagonist, and are embedded in a social context. Nostalgia is triggered by dysphoric states such as negative mood and loneliness. Finally, nostalgia generates positive affect, increases self-esteem, fosters social connectedness, and alleviates existential threat.}
}

