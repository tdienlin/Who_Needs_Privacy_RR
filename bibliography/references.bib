@dataset{dienlinPrivacyLongitudinalStudyThe2019,
  title = {The {{Privacy Longitudinal StudyThe Privacy Longitudinal Study}}},
  author = {Dienlin, Dr Philipp Masur (Project Manager), Dr Tobias, Prof Dr Sabine Trepte (PI)},
