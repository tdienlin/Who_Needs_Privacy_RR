@article{yuePassiveSocialMedia2022,
  title = {Passive Social Media Use and Psychological Well-Being during the {{COVID-19}} Pandemic: {{The}} Role of Social Comparison and Emotion Regulation},
  shorttitle = {Passive Social Media Use and Psychological Well-Being during the {{COVID-19}} Pandemic},
  author = {Yue, Zhiying and Zhang, Renwen and Xiao, Jun},
  date = {2022-02-01},
