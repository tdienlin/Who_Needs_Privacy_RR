@article{davisAccomplishingAuthenticityLaborexposing2012,
  title = {Accomplishing Authenticity in a Labor-Exposing Space},
  author = {Davis, Jenny L.},
  date = {2012-01-01T00:00:00},
  journaltitle = {Computers in Human Behavior},
  shortjournal = {Comput. Hum. Behav.},
  volume = {28},
  number = {5},
  pages = {1966--1973},
  issn = {07475632},
  doi = {10.1016/j.chb.2012.05.017},
  abstract = {The present work, through an ethnographic study of MySpace (N = 96), examines the ways in which authenticity is accomplished within a labor-exposing space. To maintain authenticity, actors must make invisible the extensive labor of self-presentation. Certain online spaces, such as social network sites and personal interactive homepages, can be thought of as labor-exposing spaces, in that they give actors clear and explicit control over self-representations, making impressions of spontaneity difficult to accomplish (Davis, 2010; Gatson, 2011a; Marwick \& boyd, 2010). I discuss and delineate several strategies used by participants to maintain authenticity on MySpace. I conclude that while the priorities of identity processes remain stable over time, the ways in which we accomplish identity are culturally, historically and materially contingent.}
}

