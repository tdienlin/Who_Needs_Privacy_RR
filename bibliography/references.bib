@article{fischerWhatMoreImportant2011,
  title = {What Is More Important for National Well-Being: Money or Autonomy? {{A}} Meta-Analysis of Well-Being, Burnout, and Anxiety across 63 Societies},
  author = {Fischer, Ronald and Boer, Diana},
  date = {2011-01-01T00:00:00},
  journaltitle = {Journal of Personality and Social Psychology},
  volume = {101},
