@article{depauloVerbalNonverbalDynamics2003,
  title = {Verbal and Nonverbal Dynamics of Privacy, Secrecy, and Deceit},
  author = {DePaulo, Bella M. and Wetzel, Chris and Weylin Sternglanz, R. and Wilson, Molly J. Walker},
  date = {2003-01-01T00:00:00},
