@article{wetzelYouReStill2020,
  title = {You’re Still so Vain: {{Changes}} in Narcissism from Young Adulthood to Middle Age},
  shorttitle = {You’re Still so Vain},
  author = {Wetzel, Eunike and Grijalva, Emily and Robins, Richard W. and Roberts, Brent W.},
