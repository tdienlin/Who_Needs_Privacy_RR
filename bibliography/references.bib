@article{liUnpackingProcessPrivacy2019,
  title = {Unpacking the Process of Privacy Management and Self-Disclosure from the Perspectives of Regulatory Focus and Privacy Calculus},
  author = {Li, Pengxiang and Cho, Hichang and Goh, Zhang Hao},
  date = {2019-08-01},
  journaltitle = {Telematics and Informatics},
  shortjournal = {Telematics and Informatics},
  volume = {41},
