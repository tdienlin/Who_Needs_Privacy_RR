@article{stefanoneContingenciesSelfworthSocialnetworkingsite2011,
  title = {Contingencies of Self-Worth and Social-Networking-Site Behavior},
  author = {Stefanone, Michael A. and Lackaff, Derek and Rosen, Devan},
  date = {2011-01-01T00:00:00},
  journaltitle = {Cyberpsychology, Behavior, and Social Networking},
  shortjournal = {Cyberpsychol. Behav. Soc. Netw.},
  volume = {14},
  number = {1-2},
  pages = {41--49},
  issn = {2152-2715},
  doi = {10.1089/cyber.2010.0049},
  abstract = {Social-networking sites like Facebook enable people to share a range of personal information with expansive groups of ‘‘friends.’’ With the growing popularity of media sharing online, many questions remain regarding antecedent conditions for this behavior. Contingencies of self-worth afford a more nuanced approach to variable traits that affect self-esteem, and may help explain online behavior. A total of 311 participants completed an online survey measuring such contingencies and typical behaviors on Facebook. First, exploratory factor analyses revealed an underlying structure to the seven dimensions of self-worth. Public-based contingencies explained online photo sharing (b¼0.158, p {$<$} 0.01), while private-based contingencies demonstrated a negative relationship with time online (b¼0.186, p {$<$} 0.001). Finally, the appearance contingency for self-worth had the strongest relationship with the intensity of online photo sharing (b¼0.242), although no relationship was evident for time spent managing profiles.},
  keywords = {Facebook}
}

@article{stefanoneExploringRelationshipPerceptions2012,
  title = {Exploring the Relationship between Perceptions of Social Capital and Enacted Support Online},
  author = {Stefanone, Michael A. and Kwon, Kyounghee Hazel and Lackaff, Derek},
  date = {2012-01-01T00:00:00},
  journaltitle = {Journal of Computer-Mediated Communication},
  shortjournal = {J. Comput. Mediat. Commun.},
  volume = {17},
  number = {4},
  pages = {451--466},
  issn = {10836101},
  doi = {10.1111/j.1083-6101.2012.01585.x},
  abstract = {Online social networking sites enable users to connect with large, heterogeneous groups of people. While extant research suggests individuals benefit psychologically from the perception that they are well connected, little is known about the nature of tangible resources embedded in these online networks. In this study 49 participants sent 588 requests for instrumental help to their Facebook friends to determine the accessibility of networked resources and online social capital. Almost 80\% of these modest requests went unanswered, and perceived bridging and bonding capital did not explain enacted support. However, people who occupied socially prestigious positions were the most likely to benefit from their friend’s help. These results suggest that expansive mediated networks may yield  limited instrumental benefits.},
  keywords = {Lesen,SNS,Social Capital}
}

