@article{sunUnderstandingThirdPersonPerception2008,
  title = {Understanding the {{Third-Person Perception}}: {{Evidence From}} a {{Meta-Analysis}}},
  shorttitle = {Understanding the {{Third-Person Perception}}},
  author = {Sun, Ye and Pan, Zhongdang and Shen, Lijiang},
  date = {2008},
  journaltitle = {Journal of Communication},
