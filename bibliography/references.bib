@article{boulianneTwentyYearsDigital2020,
  title = {Twenty {{Years}} of {{Digital Media Effects}} on {{Civic}} and {{Political Participation}}},
  author = {Boulianne, Shelley},
  date = {2020-10-01},
  journaltitle = {Communication Research},
  shortjournal = {Communication Research},
  volume = {47},
