@article{giordanoRecoveringBifactorModels2020,
  title = {Recovering Bifactor Models: {{A}} Comparison of Seven Methods},
  shorttitle = {Recovering Bifactor Models},
  author = {Giordano, Casey and Waller, Niels G.},
