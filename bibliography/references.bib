@article{kramerImpactHavingChildren2020,
  title = {The Impact of Having Children on Domain-Specific Life Satisfaction: {{A}} Quasi-Experimental Longitudinal Investigation Using the {{Socio-Economic Panel}} ({{SOEP}}) Data},
  shorttitle = {The Impact of Having Children on Domain-Specific Life Satisfaction},
  author = {Krämer, Michael D. and Rodgers, Joseph Lee},
