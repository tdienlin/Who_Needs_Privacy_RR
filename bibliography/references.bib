@article{kimFacebookPathsHappiness2011,
  title = {The {{Facebook Paths}} to {{Happiness}}: {{Effects}} of the {{Number}} of {{Facebook Friends}} and {{Self-Presentation}} on {{Subjective Well-Being}}},
  author = {Kim, Junghyun and Lee, Jong-Eun Roselyn},
  date = {2011-01-01T00:00:00},
