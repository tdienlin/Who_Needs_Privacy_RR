@article{cavaParentalCommunicationLife2014,
  title = {Parental Communication and Life Satisfaction in Adolescence},
  author = {Cava, María-Jesús and Buelga, Sofía and Musitu, Gonzalo},
  date = {2014-01-01T00:00:00},
  journaltitle = {The Spanish journal of psychology},
