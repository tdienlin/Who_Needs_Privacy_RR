@report{verbeijSelfreportedMeasuresSocial2021,
  type = {preprint},
  title = {Self-Reported Measures of Social Media Use Show High Predictive Validity},
  author = {Verbeij, Tim and Pouwels, J. Loes and Beyens, Ine and Valkenburg, Patti M.},
  date = {2021-10-29},
  institution = {{PsyArXiv}},
  doi = {10.31234/osf.io/c9bj7},
  url = {https://osf.io/c9bj7},
  urldate = {2022-04-11},
  abstract = {Existing research agrees that subjective, self-reported measures of time spent with social media (TSM) show poor convergent validity, because they correlate only modestly with equivalent objective digital trace measures. This experience sampling study among 159 adolescents (12,617 self-reports) extends this work by examining the predictive validity of self-reported TSM, that is, the extent to which self-reported and digital trace measures of TSM correspond in their effects on self-esteem, well-being, and friendship closeness. Using an advanced N=1 method of analysis, we investigated the predictive validity on a between-person, within-person, and person-specific level. Although our results confirmed the poor convergent validity of self-reported TSM reported in earlier work, we found high predictive validity of self-reported TSM for all three outcomes and all three levels. Because a high predictive validity of self-reported TSM is crucial for investigating the effects of social media, our results have important implications for future research using self-reported TSM.},
  file = {/Users/Dienlin/Zotero/storage/LVVB9FNN/Verbeij et al. - 2021 - Self-Reported Measures of Social Media Use Show Hi.pdf}
}

@article{verduynPassiveFacebookUsage2015,
  title = {Passive {{Facebook}} Usage Undermines Affective Well-Being: {{Experimental}} and Longitudinal Evidence},
  author = {Verduyn, Philippe and Lee, David Seungjae and Park, Jiyoung and Shablack, Holly and Orvell, Ariana and Bayer, Joseph and Ybarra, Oscar and Jonides, John and Kross, Ethan},
  date = {2015-01-01T00:00:00},
  journaltitle = {Journal of Experimental Psychology: General},
  volume = {144},
  number = {2},
  eprint = {25706656},
  eprinttype = {pmid},
  pages = {480--488},
  doi = {10.1037/xge0000057},
  abstract = {Prior research indicates that Facebook usage predicts declines in subjective well-being over time. How does this come about? We examined this issue in 2 studies using experimental and field methods. In Study 1, cueing people in the laboratory to use Facebook passively (rather than actively) led to declines in affective well-being over time. Study 2 replicated these findings in the field using experience-sampling techniques. It also demonstrated how passive Facebook usage leads to declines in affective well-being: by increasing envy. Critically, the relationship between passive Facebook usage and changes in affective well-being remained significant when controlling for active Facebook use, non-Facebook online social network usage, and direct social interactions, highlighting the specificity of this result. These findings demonstrate that passive Facebook usage undermines affective well-being.}
}

