  title = {Binge-Watching, Self-Determination, and Well-Being: {{A}} Partially Successful Direct Replication and Extension of {{Granow}} et al. (2018)},
  shorttitle = {Binge-{{Watching}}, {{Self-Determination}}, and {{Well-Being}}},
