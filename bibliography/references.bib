@article{reelRelationsBodyConcerns2007,
  title = {Relations of Body Concerns and Exercise Behavior: {{A}} Meta-Analysis},
  author = {Reel, Justine J. and Greenleaf, Christy and Baker, Wanda K. and Aragon, Stephen and Bishop, Dana and Cachaper, Cecile and Handwerk, Philip and Locicero, Jack and Rathburn, Lynnann and Reid, Winnie K. and Hattie, John},
  date = {2007-01-01T00:00:00},
