@article{ellisonWhyWeDon2020,
  title = {Why We Don’t Click: {{Interrogating}} the Relationship between Viewing and Clicking in Social Media Contexts by Exploring the “{{Non-Click}}”},
  shorttitle = {Why {{We Don}}’t {{Click}}},
  author = {Ellison, Nicole B and Triệu, Penny and Schoenebeck, Sarita and Brewer, Robin and Israni, Aarti},
  date = {2020-12-06},
  journaltitle = {Journal of Computer-Mediated Communication},
  shortjournal = {Journal of Computer-Mediated Communication},
  volume = {25},
  number = {6},
  pages = {402--426},
  issn = {1083-6101},
  doi = {10.1093/jcmc/zmaa013},
  url = {https://doi.org/10.1093/jcmc/zmaa013},
  urldate = {2022-04-04},
  abstract = {Motivated by work that characterizes view-based social media practices as “passive use,” contrasting it with more desirable, interactive “active use,” this study explores how social media users understand their viewing and clicking practices and the empirical relationship between them. Employing a combination of eye tracking, survey, and interview methods, our study (N\,=\,42) investigates the non-click—instances where people intentionally and thoughtfully do not click on content they spend time viewing. Counterintuitively, we find no difference in viewing duration to clicked versus non-clicked Facebook content. We find that use motivations and Facebook feed content are significant predictors of click behavior but measures of overall use, such as network size or minutes of use per day, are not. Our interview data reveal three audience-related concerns that contribute to deliberate non-clicking and illustrate how non-clicked content contributes to social connectedness when imported into other channels. We discuss implications for researchers, users, and designers.},
  file = {/Users/Dienlin/Zotero/storage/J8SIW9JN/Ellison et al. - 2020 - Why We Don’t Click Interrogating the Relationship.pdf;/Users/Dienlin/Zotero/storage/LI3F6VRP/5927572.html}
}

@article{ellisSmartphoneUsageScales2019,
  title = {Do Smartphone Usage Scales Predict Behavior?},
  author = {Ellis, David A. and Davidson, Brittany I. and Shaw, Heather and Geyer, Kristoffer},
  date = {2019-10-01},
  journaltitle = {International Journal of Human-Computer Studies},
  shortjournal = {International Journal of Human-Computer Studies},
  volume = {130},
  pages = {86--92},
  issn = {1071-5819},
  doi = {10.1016/j.ijhcs.2019.05.004},
  url = {http://www.sciencedirect.com/science/article/pii/S1071581919300473},
  urldate = {2020-07-14},
  abstract = {Understanding how people use technology remains important, particularly when measuring the impact this might have on individuals and society. However, despite a growing body of resources that can quantify smartphone use, research within psychology and social science overwhelmingly relies on self-reported assessments. These have yet to convincingly demonstrate an ability to predict objective behavior. Here, and for the first time, we compare a variety of smartphone use and ‘addiction’ scales with objective behaviors derived from Apple's Screen Time application. While correlations between psychometric scales and objective behavior are generally poor, single estimates and measures that attempt to frame technology use as habitual rather than ‘addictive’ correlate more favorably with subsequent behavior. We conclude that existing self-report instruments are unlikely to be sensitive enough to accurately predict basic technology use related behaviors. As a result, conclusions regarding the psychological impact of technology are unreliable when relying solely on these measures to quantify typical usage.},
  langid = {english},
  annotation = {00058},
  file = {/Users/Dienlin/Zotero/storage/IWN3AR3B/Ellis et al. - 2019 - Do smartphone usage scales predict behavior.pdf;/Users/Dienlin/Zotero/storage/U6YAFCYF/S1071581919300473.html}
}

