  date = {2011-01-01T00:00:00},
  journaltitle = {Cyberpsychology, behavior and social networking},
  shortjournal = {Cyberpsychol Behav Soc Netw},
  volume = {14},
  number = {4},
  eprint = {21067280},
  eprinttype = {pmid},
  pages = {253--258},
  issn = {2152-2723},
  doi = {10.1089/cyber.2010.0161},
  abstract = {The effect of Internet use on social relationships is still a matter of intense debate. This study examined the relationships between use of social media (instant messaging and social network sites), network size, and emotional closeness in a sample of 117 individuals aged 18 to 63 years old. Time spent using social media was associated with a larger number of online social network "friends." However, time spent using social media was not associated with larger offline networks, or feeling emotionally closer to offline network members. Further, those that used social media, as compared to non-users of social media, did not have larger offline networks, and were not emotionally closer to offline network members. These results highlight the importance of considering potential time and cognitive constraints on offline social networks when examining the impact of social media use on social relationships.}
}

