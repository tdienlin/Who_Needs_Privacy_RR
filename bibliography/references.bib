@article{ruddigkeitDingeMeineEltern2013,
  title = {Dinge, Die Meine {{Eltern}} Nicht Sehen Sollten},
  author = {Ruddigkeit, A. and Penzel, J. and Schneider, J.},
  date = {2013-01-01T00:00:00},
  journaltitle = {Publizistik},
  volume = {58},
  number = {3},
  pages = {305--325},
  issn = {0033-4006},
  doi = {10.1007/s11616-013-0183-z},
  abstract = {Jeder erfolgreiche Kommunikationsprozess setzt einen bestimmten Grad an Selbstauskunft gegenüber anderen voraus. Und wie auch im realen Leben bedeutet Selbstauskunft auf SNS wie Facebook trotzdem nicht die Aufgabe jeglicher Privatsphäre, sondern stattdessen sind bestimmte als schützend wahrgenommene Grenzziehungen die Voraussetzung zur Selbstauskunft. Dieses Grundprinzip kann in unterschiedlichsten Strategien umgesetzt werden. Eine Onlinebefragung unter deutschen Intensivnutzern (n=684) prüfte diese Annahme der konditionalen Selbstauskunft. Ausgehend von Burgoons (1982) Unterscheidung vier verschiedener Dimensionen der Privatsphäre, wurde eine Skala zur subjektiven Nutzungspraxis entwickelt. Die unterschiedliche Gewichtung der dabei gemessenen Privacydimensionen ermöglichte es, unter unseren Befragten sieben Typen der Privacyregulierung zu unterscheiden. Konditionale Selbstauskunft lag bei sechs von sieben Nutzertypen (DIE MITTEILSAME, BLOGGER, DER NETZWERKER, DIE GRUPPENSORTIERERIN, DIE PASSIVE und INTERNETUNERFAHRENE) vor. Dagegen agierte der Typ DES BEDENKENLOSEN entgegen den Erwartungen und knüpfte Selbstauskunft an keine der gemessenen Bedingungen.}
}

